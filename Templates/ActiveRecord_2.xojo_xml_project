   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub BeginTransaction()</SourceLine>
   <SourceLine>break //Should be called by the Database Adapter Subclass</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
