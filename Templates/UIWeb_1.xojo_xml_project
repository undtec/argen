  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function BuildPreparedStatement(bAsCount as Boolean = false, iOffset as integer = -1) As PreparedSQLStatement</SourceLine>
   <SourceLine>dim arsSQL() as String</SourceLine>
   <SourceLine>arsSQL.Append(DataFile.t_Table.BaseSQL(bAsCount))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Set up search</SourceLine>
   <SourceLine>dim ariBindTypes() as Integer</SourceLine>
   <SourceLine>dim arvBindValues() as Variant</SourceLine>
   <SourceLine>dim sSearch as String = ctlSearch.SearchString.Trim</SourceLine>
   <SourceLine>dim bHasSearch as Boolean = (sSearch &lt;&gt; "")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Configure the search query</SourceLine>
   <SourceLine>if bHasSearch then</SourceLine>
   <SourceLine>// Bind the PreparedStatement types and values and they will be handled automatically</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// arsSQL.Append("WHERE {field} LIKE ?")</SourceLine>
   <SourceLine>// ariBindTypes.Append(PreparedSQLStatement.TYPE)</SourceLine>
   <SourceLine>// arvBindValues.Append("%" + sSearch + "%")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if msSortedColumn.Trim &lt;&gt; "" then</SourceLine>
   <SourceLine>arsSQL.Append("ORDER BY " + msSortedColumn)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>#if (not TP_ActiveRecord.kConfigUseMSSQLServer) and (not TP_ActiveRecord.kConfigUseMSSQLServerMBS) then</SourceLine>
   <SourceLine>// MS SQL server syntax is different, you will need to implement it.</SourceLine>
   <SourceLine>// MS SQL *REQUIRES* an ORDER BY to do this and ARGen supports "no default sort"</SourceLine>
   <SourceLine>// So this is up to you to resolve if you wish to use this feature.</SourceLine>
   <SourceLine>// https://www.sqlservertutorial.net/sql-server-basics/sql-server-offset-fetch/</SourceLine>
   <SourceLine>if iOffset &gt; -1 and bAsCount = false then</SourceLine>
   <SourceLine>arsSQL.Append("LIMIT " + Format(DataFile.kMaxReturn, "#########"))</SourceLine>
   <SourceLine>arsSQL.Append("OFFSET " + Format(iOffset, "#########"))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>#endif</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Join SQL and Prepare it</SourceLine>
   <SourceLine>dim sSQL as String = Join(arsSQL, " ")</SourceLine>
   <SourceLine>dim ps as PreparedSQLStatement = DataFile.DB.Prepare(sSQL)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Check bind errors</SourceLine>
   <SourceLine>if ariBindTypes.Ubound &lt;&gt; arvBindValues.Ubound then</SourceLine>
   <SourceLine>dim ex as new OutOfBoundsException</SourceLine>
   <SourceLine>ex.Message = "Bind types and bind values do not match"</SourceLine>
   <SourceLine>raise ex</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Handle binds</SourceLine>
   <SourceLine>for tiBind as Integer = 0 to ariBindTypes.Ubound</SourceLine>
   <SourceLine>ps.BindType(tiBind, ariBindTypes(tiBind))</SourceLine>
   <SourceLine>ps.Bind(tiBind, arvBindValues(tiBind))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next tiBind</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return ps</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>bAsCount as Boolean = false, iOffset as integer = -1</ItemParams>
  <ItemResult>PreparedSQLStatement</ItemResult>
 </Method>
 <Method>
  <ItemName>EnableButtons</ItemName>
