 <Method>
  <ItemName>SelectedRecords</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1800243199</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function SelectedRecords() As DataFile.vw_View()</SourceLine>
   <SourceLine>dim aroSelection() as DataFile.vw_View</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// No selection</SourceLine>
   <SourceLine>if lbRows.ListIndex &lt; 0 then</SourceLine>
   <SourceLine>return aroSelection</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Selection</SourceLine>
   <SourceLine>dim iMax as Integer = (lbRows.RowCount - 1)</SourceLine>
   <SourceLine>for i as Integer = 0 to iMax</SourceLine>
   <SourceLine>if lbRows.Selected(i) then</SourceLine>
   <SourceLine>aroSelection.Append(lbRows.RowTag(i))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next i</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return aroSelection</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult>DataFile.vw_View()</ItemResult>
 </Method>
 <Method>
  <ItemName>LoadList</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>901060607</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub LoadList(bRefershPagination as Boolean = false)</SourceLine>
   <SourceLine>lbRows.DeleteAllRows</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if bRefershPagination or miCurrentPage = -1 then</SourceLine>
   <SourceLine>dim psPagination as PreparedSQLStatement = BuildPreparedStatement(true)</SourceLine>
   <SourceLine>miTotalItems = DataFile.t_Table.ListCount(psPagination)</SourceLine>
   <SourceLine>miMaxPages = Max(1, Ceil(miTotalItems / DataFile.kMaxReturn))</SourceLine>
   <SourceLine>miCurrentPage = 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dim iOffset as Integer = Max(0, (miCurrentPage - 1) * DataFile.kMaxReturn)</SourceLine>
   <SourceLine>dim ps as PreparedSQLStatement = BuildPreparedStatement(false, iOffset)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dim aroRecords() as DataFile.t_Table = DataFile.t_Table.List(ps)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>for each oRecord as DataFile.t_Table in aroRecords</SourceLine>
   <SourceLine>lbRows.AddRow(LoadListBuildRow(oRecord))</SourceLine>
   <SourceLine>lbRows.RowTag(lbRows.LastIndex) = oRecord</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next oRecord</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>ctlPaging.iCurrentPage = miCurrentPage</SourceLine>
   <SourceLine>ctlPaging.iMaxPage = miMaxPages</SourceLine>
   <SourceLine>ctlPaging.iTotalItems = miTotalItems</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Enable search</SourceLine>
   <SourceLine>ctlSearch.SetEnabled = true</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>bRefershPagination as Boolean = false</ItemParams>
  <ItemResult></ItemResult>
 </Method>
 <Method>
  <ItemName>HandleView</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1967675391</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub HandleView()</SourceLine>
   <SourceLine>dim aroRecords() as DataFile.vw_View = SelectedRecords</SourceLine>
   <SourceLine>if aroRecords.Ubound &lt; 0 then return</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>for each oRecord as DataFile.vw_View in aroRecords</SourceLine>
   <SourceLine>dim oWin as new winvw_ViewView</SourceLine>
   <SourceLine>oWin.Load(oRecord)</SourceLine>
   <SourceLine>oWin.Show</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next oRecord</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult></ItemResult>
 </Method>
