 <ObjName>MSSQLServerAdapterMBS</ObjName>
 <ObjContainerID>1972432888</ObjContainerID>
 <IsClass>1</IsClass>
 <Superclass>TP_ActiveRecord.DatabaseAdapter</Superclass>
