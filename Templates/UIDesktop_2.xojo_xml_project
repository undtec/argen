  <ItemResult>String()</ItemResult>
 </Method>
 <Method>
  <ItemName>LoadListBuildSort</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1627977727</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function LoadListBuildSort() As String</SourceLine>
   <SourceLine>// No sort</SourceLine>
   <SourceLine>var iSortedColumn as Integer = lbRows.SortingColumn</SourceLine>
   <SourceLine>if iSortedColumn &lt; 0 then return ""</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Get field</SourceLine>
   <SourceLine>var sSortField as String = lbRows.ColumnTagAt(iSortedColumn)</SourceLine>
   <SourceLine>if sSortField = "" then return ""</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Return sort direction</SourceLine>
   <SourceLine>select case lbRows.ColumnSortDirectionAt(iSortedColumn)</SourceLine>
   <SourceLine>case Listbox.SortDirections.Ascending</SourceLine>
   <SourceLine>return sSortField + " ASC"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case Listbox.SortDirections.Descending</SourceLine>
   <SourceLine>return sSortField + " DESC"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>case Listbox.SortDirections.None</SourceLine>
   <SourceLine>// No value</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end select</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult>String</ItemResult>
 </Method>
 <Method>
  <ItemName>SelectedRecords</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>292276223</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function SelectedRecords() As DataFile.t_Table()</SourceLine>
   <SourceLine>var aroSelection() as DataFile.t_Table</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// No selection</SourceLine>
   <SourceLine>if lbRows.SelectedRowIndex &lt; 0 then</SourceLine>
   <SourceLine>return aroSelection</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Selection</SourceLine>
   <SourceLine>var iMax as Integer = (lbRows.RowCount - 1)</SourceLine>
   <SourceLine>for i as Integer = 0 to iMax</SourceLine>
   <SourceLine>if lbRows.Selected(i) then</SourceLine>
   <SourceLine>aroSelection.Add(lbRows.RowTagAt(i))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next i</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return aroSelection</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult>DataFile.t_Table()</ItemResult>
 </Method>
