  <Visible>1</Visible>
  <PartID>731273215</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub EnableButtons()</SourceLine>
   <SourceLine>dim bSelection as Boolean = (lbRows.ListIndex &gt; -1)</SourceLine>
   <SourceLine>ctlAdd.SetAddEnabled = true</SourceLine>
   <SourceLine>ctlAdd.SetEditEnabled = bSelection</SourceLine>
   <SourceLine>ctlAdd.SetDeleteEnabled = bSelection</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult></ItemResult>
 </Method>
 <Method>
  <ItemName>HandleDelete</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1666215935</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub HandleDelete()</SourceLine>
   <SourceLine>dim aroSelection() as DataFile.t_Table = SelectedRecords</SourceLine>
   <SourceLine>if aroSelection.Ubound &lt; 0 then return</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dim aroErrs() as TP_ActiveRecord.ValidationErrors</SourceLine>
   <SourceLine>for each oRecord as DataFile.t_Table in aroSelection</SourceLine>
   <SourceLine>dim oErr as new TP_ActiveRecord.ValidationErrors</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Validate before deletion</SourceLine>
   <SourceLine>if oRecord.Validate(oErr) = false then</SourceLine>
   <SourceLine>aroErrs.Append(oErr)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next oRecord</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Handle errors</SourceLine>
   <SourceLine>if aroErrs.Ubound &gt; -1 then</SourceLine>
   <SourceLine>break</SourceLine>
   <SourceLine>return</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dim oConfirm as new dlgTPConfirmDelete</SourceLine>
   <SourceLine>oConfirm.delDelete = WeakAddressOf HandleDeleteConfirm</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>oConfirm.Show</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult></ItemResult>
 </Method>
 <Method>
  <ItemName>HandleEdit</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1328623615</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub HandleEdit()</SourceLine>
   <SourceLine>// This example is only designed to edit one record at a time</SourceLine>
   <SourceLine>dim aroRecords() as DataFile.t_Table = SelectedRecords</SourceLine>
   <SourceLine>if aroRecords.Ubound &lt;&gt; 0 then</SourceLine>
   <SourceLine>break</SourceLine>
   <SourceLine>return</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dim oRecord as DataFile.t_Table = aroRecords(0)</SourceLine>
   <SourceLine>dim oWin as new wint_TableAddEdit</SourceLine>
   <SourceLine>oWin.Load(oRecord)</SourceLine>
   <SourceLine>oWin.oParent = self</SourceLine>
   <SourceLine>oWin.Show</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult></ItemResult>
 </Method>
 <Method>
