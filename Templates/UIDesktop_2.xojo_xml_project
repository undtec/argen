   <SourceLine>Function LoadListBuildRow(oRecord as DataFile.t_Table) As String()</SourceLine>
   <SourceLine>var arsRow() as String</SourceLine>
   <SourceLine>return arsRow</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
