 <Method>
  <ItemName>BuildPreparedStatement</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>2057486335</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function BuildPreparedStatement() As PreparedSQLStatement</SourceLine>
   <SourceLine>var arsSQL() as String</SourceLine>
   <SourceLine>arsSQL.Add(DataFile.vw_View.BaseSQL)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Set up search</SourceLine>
   <SourceLine>var ariBindTypes() as Integer</SourceLine>
   <SourceLine>var arvBindValues() as Variant</SourceLine>
   <SourceLine>var sSearch as String = txtSearch.Text.Trim</SourceLine>
   <SourceLine>var bHasSearch as Boolean = (sSearch &lt;&gt; "")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Configure the search query</SourceLine>
   <SourceLine>if bHasSearch then</SourceLine>
   <SourceLine>// Bind the PreparedStatement types and values and they will be handled automatically</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// arsSQL.Add("WHERE {field} LIKE ?")</SourceLine>
   <SourceLine>// ariBindTypes.Add(PreparedSQLStatement.TYPE)</SourceLine>
   <SourceLine>// arvBindValues.Add("%" + sSearch + "%")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>var sSort as String = LoadListBuildSort</SourceLine>
   <SourceLine>if sSort.Trim &lt;&gt; "" then</SourceLine>
   <SourceLine>arsSQL.Add("ORDER BY " + sSort)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Join SQL and Prepare it</SourceLine>
   <SourceLine>var sSQL as String = String.FromArray(arsSQL, " ")</SourceLine>
   <SourceLine>var ps as PreparedSQLStatement = DataFile.DB.Prepare(sSQL)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Check bind errors</SourceLine>
   <SourceLine>if ariBindTypes.LastIndex &lt;&gt; arvBindValues.LastIndex then</SourceLine>
   <SourceLine>var ex as new OutOfBoundsException</SourceLine>
   <SourceLine>ex.Message = "Bind types and bind values do not match"</SourceLine>
   <SourceLine>raise ex</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Handle binds</SourceLine>
   <SourceLine>for tiBind as Integer = 0 to ariBindTypes.LastIndex</SourceLine>
   <SourceLine>ps.BindType(tiBind, ariBindTypes(tiBind))</SourceLine>
   <SourceLine>ps.Bind(tiBind, arvBindValues(tiBind))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next tiBind</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return ps</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult>PreparedSQLStatement</ItemResult>
