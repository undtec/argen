 <ControlBehavior>
  <Superclass>WebTextField</Superclass>
  <HookInstance>
   <ItemName>KeyPressed</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>1232064511</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub KeyPressed(Details As REALbasic.KeyEvent)</SourceLine>
    <SourceLine>if details.KeyCode &lt;&gt; details.KeyEnter then Return</SourceLine>
    <SourceLine>if self.Enabled = false then Return</SourceLine>
    <SourceLine>if me.Enabled = false then Return</SourceLine>
    <SourceLine>self.Enabled = false</SourceLine>
    <SourceLine>dim iVal as integer = me.text.val</SourceLine>
    <SourceLine>if iVal &lt; 1 then</SourceLine>
    <SourceLine>iVal = 1</SourceLine>
    <SourceLine>me.Text = "1"</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>if iVal &gt; miMaxPage then</SourceLine>
    <SourceLine>iVal = miMaxPage</SourceLine>
    <SourceLine>me.Text = str(miMaxPage)</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>RaiseEvent GoToPage iVal</SourceLine>
    <SourceLine>UpdateUI</SourceLine>
    <SourceLine>self.Enabled = true</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
  <HookInstance>
