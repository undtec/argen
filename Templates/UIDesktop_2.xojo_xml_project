 <Method>
  <ItemName>HandleDelete</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>970389503</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub HandleDelete()</SourceLine>
   <SourceLine>var aroSelection() as DataFile.t_Table = SelectedRecords</SourceLine>
   <SourceLine>if aroSelection.LastIndex &lt; 0 then return</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>var md as new MessageDialog</SourceLine>
   <SourceLine>md.Message = Strings.DeleteRecords_</SourceLine>
   <SourceLine>md.Explanation = Strings.DeleteAreYouSure</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>md.ActionButton.Caption = Strings.Delete</SourceLine>
   <SourceLine>md.CancelButton.Visible = true</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// User cancelled</SourceLine>
   <SourceLine>if md.ShowModal &lt;&gt; md.ActionButton then return</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>var aroErrs() as TP_ActiveRecord.ValidationErrors</SourceLine>
   <SourceLine>for each oRecord as DataFile.t_Table in aroSelection</SourceLine>
   <SourceLine>var oErr as new TP_ActiveRecord.ValidationErrors</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Validate before deletion</SourceLine>
   <SourceLine>if oRecord.Validate(oErr) = false then</SourceLine>
   <SourceLine>aroErrs.Add(oErr)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>oRecord.Delete</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next oRecord</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Handle errors</SourceLine>
   <SourceLine>if aroErrs.LastIndex &gt; -1 then</SourceLine>
   <SourceLine>break</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>LoadList</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult></ItemResult>
 </Method>
 <Method>
  <ItemName>HandleEdit</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>256989183</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub HandleEdit()</SourceLine>
   <SourceLine>// This example is only designed to edit one record at a time</SourceLine>
   <SourceLine>var aroRecords() as DataFile.t_Table = SelectedRecords</SourceLine>
   <SourceLine>if aroRecords.LastIndex &lt;&gt; 0 then</SourceLine>
   <SourceLine>break</SourceLine>
   <SourceLine>return</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>var oRecord as DataFile.t_Table = aroRecords(0)</SourceLine>
   <SourceLine>RaiseEvent EditRecord(oRecord)</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
