   <SourceLine>iDBType = ODBCPreparedStatement.ODBC_TYPE_BIGINT</SourceLine>
   <SourceLine>case else</SourceLine>
   <SourceLine>break 'unsupported type</SourceLine>
   <SourceLine>end select</SourceLine>
