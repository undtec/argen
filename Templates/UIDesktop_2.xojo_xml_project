 </Method>
 <Method>
  <ItemName>LoadList</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1945075711</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub LoadList()</SourceLine>
   <SourceLine>// Maintain selection</SourceLine>
   <SourceLine>var ariSelectedIDs() as Integer</SourceLine>
   <SourceLine>for iRow as Integer = lbRows.RowCount - 1 downto 0</SourceLine>
   <SourceLine>if lbRows.Selected(iRow) then</SourceLine>
   <SourceLine>var oTag as DataFile.t_Table = lbRows.RowTagAt(iRow)</SourceLine>
   <SourceLine>if oTag &lt;&gt; nil then</SourceLine>
   <SourceLine>ariSelectedIDs.Add(oTag.ID)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next iRow</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Clear</SourceLine>
   <SourceLine>lbRows.RemoveAllRows</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Configure columns</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>var ps as PreparedSQLStatement = BuildPreparedStatement</SourceLine>
   <SourceLine>var aroRows() as DataFile.t_Table = DataFile.t_Table.List(ps)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Load listbox</SourceLine>
   <SourceLine>for each oRow as DataFile.t_Table in aroRows</SourceLine>
   <SourceLine>lbRows.AddRow(LoadListBuildRow(oRow))</SourceLine>
   <SourceLine>lbRows.RowTagAt(lbRows.LastAddedRowIndex) = oRow</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Restore selection</SourceLine>
   <SourceLine>if ariSelectedIDs.LastIndex &gt; -1 then</SourceLine>
   <SourceLine>lbRows.Selected(lbRows.LastAddedRowIndex) = (ariSelectedIDs.IndexOf(oRow.ID) &gt; -1)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>next oRow</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>EnableButtons</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams></ItemParams>
  <ItemResult></ItemResult>
 </Method>
 <Method>
