 <ControlBehavior>
  <Superclass>WebListBox</Superclass>
  <HookInstance>
   <ItemName>DoubleClick</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>229345279</PartID>
