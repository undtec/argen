   <ItemName>HeaderPressed</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>626735103</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub HeaderPressed(Column as Integer)</SourceLine>
    <SourceLine>dim sPreviousSort as String = msSortedColumn</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>select case Column</SourceLine>
    <SourceLine>end select</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>// Check for descending sort</SourceLine>
    <SourceLine>if sPreviousSort = msSortedColumn then</SourceLine>
    <SourceLine>msSortedColumn = msSortedColumn + " DESC"</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>end</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>LoadList(true)</SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
